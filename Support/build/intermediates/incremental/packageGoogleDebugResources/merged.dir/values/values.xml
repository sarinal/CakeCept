<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="chat_bubble_text_incoming">#262626</color>
    <color name="darkgreyText">#686868</color>
    <color name="divider_color">#C8C8C8</color>
    <color name="high_priority_message">#ff3333</color>
    <color name="low_priority_message">#4db3db</color>
    <color name="primaryColor">#087099</color>
    <color name="settingsLabelColor">#000000</color>
    <color name="sliding_tab_background">#F5F5F5</color>
    <declare-styleable name="MaxWidthLinearLayout">
        <attr format="dimension" name="maxWidth"/>
    </declare-styleable>
    <declare-styleable name="Setting">
        <attr format="string" name="settingLabel"/>
        <attr format="string" name="settingSummary"/>
        <!--switch style only valid for Settings with a checkbox/switch-->
        <!--true implies switchCompat, false implies checkBox-->
        <attr format="boolean" name="settingSwitch"/>
        <attr format="boolean" name="hideDivider"/>
    </declare-styleable>
    <declare-styleable name="StateImageView">
        <attr format="boolean" name="state_new"/>
    </declare-styleable>
    <dimen name="conversation_chat_background_corner_radius">6dp</dimen>
    <dimen name="conversation_chat_bubble_avatar_size">40dp</dimen>
    <dimen name="conversation_chat_bubble_avatar_top_padding">20dp</dimen>
    <dimen name="conversation_chat_bubble_incoming_status_margin_R">4dp</dimen>
    <dimen name="conversation_chat_bubble_incoming_status_margin_right">4dp</dimen>
    <dimen name="conversation_chat_bubble_margin_incoming">16dp</dimen>
    <dimen name="conversation_chat_bubble_margin_outgoing">60dp</dimen>
    <dimen name="conversation_chat_bubble_max_width">400dp</dimen>
    <dimen name="conversation_chat_bubble_max_width_outgoing">444dp</dimen>
    <dimen name="conversation_chat_bubble_outgoing_status_margin_R">4dp</dimen>
    <dimen name="conversation_chat_bubble_padding">6dp</dimen>
    <dimen name="conversation_chat_date_header_spacing_H">8dp</dimen>
    <dimen name="conversation_chat_row_padding_B">2dp</dimen>
    <dimen name="conversation_chat_row_padding_H">16dp</dimen>
    <dimen name="conversation_chat_row_padding_T">8dp</dimen>
    <dimen name="conversation_chat_timestamp_padding_B">2dp</dimen>
    <dimen name="fontSizeMedium">16sp</dimen>
    <dimen name="priority_margin_end">-3dp</dimen>
    <dimen name="priority_width">15dp</dimen>
    <string name="OK">OK</string>
    <string name="app_name">BbmSdkSupport</string>
    <string name="ask_again_button">Ask Again</string>
    <string name="button_continue">Continue</string>
    <string name="cancel">Cancel</string>
    <string name="confirm_your_password_hint">Confirm new password</string>
    <string name="do_not_ask_now_button">OK</string>
    <string name="existing_password_hint">Enter your password</string>
    <string name="forgot_password">Forgot Password</string>
    <string name="forgot_password_warning">Resetting your application password will log you out of your account and remove you from all chats. Are you sure you want to continue?</string>
    <string name="incorrect_password">The entered password is incorrect. Please enter the correct password.</string>
    <string name="key_change_temp_failure">Temporary failure setting new password. Please try again.</string>
    <string name="new_password_hint">Enter a new password</string>
    <string name="new_password_text">Enter a password. This will be use to secure your chats.</string>
    <string name="password_cant_be_changed">Unable to reach the password service, try again later.</string>
    <string name="password_not_valid">Password was not a valid password.</string>
    <string name="passwords_do_not_match">Password confirmation does not match.</string>
    <string name="permission_denied_title">Permission denied</string>
    <string name="permission_request_title">App permission</string>
    <string name="provide_existing_password">Your application password is required</string>
    <string name="provide_new_password">Please choose a password.</string>
    <string name="settings">Settings</string>
    <string name="sync_failure">Failed to sync security keys. Please try again.</string>
    <string name="sync_timeout">Security keys recovery timed out. Please try again.</string>
    <string name="timestamp_format_just_now">Just now</string>
    <string name="timestamp_format_minutes_ago">%1$s minutes ago</string>
    <string name="timestamp_format_one_minute_ago">1 minute ago</string>
    <style name="AppSplashDialog" parent="BBMSdkSupport.darkdialog">
        <!--
            The Network Connected dialog is shown in an activity that has set a value
            for the android:background item. In order to avoid displaying the
            same background in the dialog, set that background to @null.
        -->
        <item name="android:background">@null</item>
    </style>
    <style name="BBMSdkSupport.darkdialog" parent="Theme.AppCompat.Dialog">
        <item name="colorAccent">#087099</item>
    </style>
    <style name="ConversationDate" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/darkgreyText</item>
        <item name="android:textSize">11sp</item>
    </style>
    <style name="ConversationSender" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/chat_bubble_text_incoming</item>
        <item name="android:textSize">13sp</item>
    </style>
    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>
    <style name="SettingLabel">
        <item name="android:textSize">@dimen/fontSizeMedium</item>
        <item name="android:textColor">@color/settingsLabelColor</item>
    </style>
</resources>